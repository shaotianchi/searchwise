{
  "manifest_version": 3,
  "name": "Searchwise",
  "description": "Make your search more wiser",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "vite.svg"
  },
  "content_scripts": [
    {
      "js": ["src/scripts/content.tsx"],
      "matches": ["<all_urls>"],
      "run_at": "document_end"
    }
  ]
}
